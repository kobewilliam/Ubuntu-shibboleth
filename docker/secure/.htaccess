#LoadModule rewrite_module /usr/lib/apache2/modules/mod_rewrite.so
<IfModule mod_rewrite.c>
RewriteEngine On

RewriteBase /dist/

RewriteRule ^index.html$ - [L]

RewriteCond %{REQUEST_FILENAME} !-f

RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule . /dist/index.html [L]

</IfModule>

